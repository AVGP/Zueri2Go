<?xml version='1.0' encoding='utf-8'?>
<widget id="de.geekonaut.zueri2go" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Zueri2Go</name>

    <description>
      Helps you find panoramic views and playgrounds around you.
    </description>

    <author email="mr.avgp@gmail.com" href="http://www.geekonaut.de">
      Martin Naumann
    </author>
    
    <content src="index.html" />

    <access origin="*" />

    <icon src="icon.png" />

    <feature name="http://api.phonegap.com/1.0/geolocation"/>
    <feature name="http://api.phonegap.com/1.0/network"/>
</widget>
